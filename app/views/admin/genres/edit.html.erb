<h1>（管理者側）ジャンル編集</h1>
<br></br>
<table class="table table-hover table-inverse">
  <thead>
    <tr>
      <th>ジャンル名</th>
        <%= form_with model: @genre,url: edit_admin_genre_path,local: true do |f|%>
      <th>
        <%= f.text_field :name %>
      </th>
      <th>
        <%= f.submit '変更を保存', class: "btn btn-success" %>
      </th>
        <% end %>
    </tr>
  </thead>
</table>